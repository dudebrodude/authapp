<% provide(:title, "Sign in") %>


<div class="row">
  <div class="col-sm-4">
    <h1>Sign in</h1>
    <%= form_for(:session, url: sessions_path) do |f| %>

       <%= f.text_field :email %>

      <%= f.password_field :password %>

      <%= f.submit "Sign in", class: "btn btn-large btn-primary" %>
    <% end %>
    </div>
  <div class="col-sm-8">
 <h1>Haven't Signed Up Yet?</h1>
  </div>
</div>