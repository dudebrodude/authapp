<% provide(:title, @title) %>
<div class="row">
  <aside class="span4">
    <section>
   
      <h1><%= @club.name %></h1>
    </section>
    <section>
      <%= render 'shared/stats' %>
      <% if @clubs.any? %>

      <% end %>
    </section>
  </aside>
  <div class="span8">
    <h3><%= @title %></h3>
    <% if @clubs.any? %>
      <ul class="users">
        <%= render @clubs %>
      </ul>
      <%= will_paginate %>
    <% end %>
  </div>
</div>